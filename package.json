{
  "name": "fotm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "env-cmd -f .env.local next build",
    "local": "env-cmd -f .env.local next dev -p 3000",
    "build:dev": "env-cmd -f .env.dev next build",
    "dev": "env-cmd -f .env.dev next start -p 4000",
    "build:stg": "env-cmd -f .env.prd next build",
    "stg": "env-cmd -f .env.prd next start -p 5000",
    "build:prd": "env-cmd -f .env.prd next build",
    "prd": "env-cmd -f .env.prd next start -p 6000"
  },
  "dependencies": {
    "@codemirror/basic-setup": "^0.20.0",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@uiw/react-codemirror": "^4.23.7",
    "codemirror": "^6.0.1",
    "env-cmd": "^10.1.0",
    "next": "^14.2.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vue-codemirror": "^6.1.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
